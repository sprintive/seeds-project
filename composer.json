{
  "name": "qanneh/seeds-project",
  "description": "Kickoff distro for SME's, Curated by Sprintive.",
  "type": "project",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "drupal/drupal-extension": "~3.3.0",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "drupal/coder": "8.*",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "~2.8",
    "se/selenium-server-standalone": "^2.53",
    "composer/composer": "^1.3",
    "drush/drush": "^9.0",
    "drupal/console": "1.0.1"
  },
  "require": {
    "drupal-composer/drupal-scaffold": "2.3.0",
    "cweagans/composer-patches": "^1.6.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "webflo/drupal-finder": "1.0",
    "webmozart/path-util": "2.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "optimize-autoloader": true
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "@composer drupal-scaffold"
    ],
    "post-update-cmd": [
      "@composer drupal-scaffold"
    ]
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-2.x": "8.2.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "public_html/core": [
        "type:drupal-core"
      ],
      "public_html/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": false
  },
  "authors": [
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    },
    {
      "name": "Mohammad AlQanneh",
      "homepage": "https://www.drupal.org/u/mqanneh",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/seeds",
    "source": "http://cgit.drupalcode.org/seeds"
  }
}
