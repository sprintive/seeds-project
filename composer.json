{
  "name": "sprintive/seeds-project",
  "description": "Kickoff distro for SME's, Curated by Sprintive.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "^1.6.4",
    "drupal-composer/drupal-scaffold": "2.3.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "webflo/drupal-finder": "1.0",
    "webmozart/path-util": "2.3",
    "sprintive/seeds": "^8.8.2",
    "drupal/coder": "8.*",
    "symfony/css-selector": "~2.8",
    "composer/composer": "^1.3",
    "drush/drush": "~8.1",
    "drupal/console": "1.0.1",
    "kenwheeler/slick": "1.6.0",
    "gdsmith/easing": "1.4.1",
		"dinbror/blazy": "1.8.2",
		"dimsemenov/photoswipe": "4.1.3",
		"ajaxorg/ace-builds": "1.4",
		"ckeditor-plugin/fakeobjects": "4.14.0",
		"ckeditor-plugin/iframe": "4.14.0"
  },
	"patchLevel": {
		"drupal/core": "-p2"
	},
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "kenwheeler/slick",
        "version": "1.6.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/kenwheeler/slick/archive/1.6.0.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "gdsmith/easing",
        "version": "1.4.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/gdsmith/jquery.easing/archive/1.4.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "dinbror/blazy",
        "version": "1.8.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
		},
		{
			"type": "package",
			"package": {
				"name": "dimsemenov/photoswipe",
				"version": "4.1.3",
				"type": "drupal-library",
				"dist": {
					"url": "https://github.com/dimsemenov/PhotoSwipe/archive/v4.1.3.zip",
					"type": "zip"
				},
				"require": {
					"composer/installers": "~1.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "ajaxorg/ace-builds",
				"version": "1.4",
				"type": "drupal-library",
				"dist": {
					"url": "https://github.com/ajaxorg/ace-builds/archive/v1.4.0.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "ckeditor-plugin/fakeobjects",
				"version": "4.14.0",
				"type": "drupal-library",
				"dist": {
					"url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.14.0.zip",
					"type": "zip"
				},
				"require": {
					"composer/installers": "~1.0"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "ckeditor-plugin/iframe",
				"version": "4.14.0",
				"type": "drupal-library",
				"dist": {
					"url": "https://download.ckeditor.com/iframe/releases/iframe_4.14.0.zip",
					"type": "zip"
				},
				"require": {
					"composer/installers": "~1.0"
				}
			}
		}
  ],
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "optimize-autoloader": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "@composer drupal-scaffold",
      "Seeds\\composer\\ScriptHandler::removeGitDirectories"
    ],
    "post-update-cmd": [
      "@composer drupal-scaffold",
      "Seeds\\composer\\ScriptHandler::removeGitDirectories"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-7.x": "8.7.x-dev"
		},
		"installer-types": [
			"npm-asset"
		],
    "installer-paths": {
      "public_html/core": [
        "type:drupal-core"
      ],
      "public_html/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
			],
			"public_html/libraries/ace": [
				"npm-asset/ace-builds"
			],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": false
  },
  "authors": [
    {
      "name": "Sprintive",
      "homepage": "http://sprintive.com",
      "role": "Supporting Organization"
    },
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/seeds",
    "source": "http://cgit.drupalcode.org/seeds",
    "email": "info@sprintive.com"
  }
}
